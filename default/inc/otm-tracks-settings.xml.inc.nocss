<!ENTITY yellow_green_hiking_filter "<Filter>%yellow_track; and not %red_track; and %green_track; and not %blue_track;</Filter>">
<!ENTITY yellow_blue_hiking_filter "<Filter>%yellow_track; and not %red_track; and not %green_track; and %blue_track;</Filter>">
<!ENTITY red_green_hiking_filter "<Filter>not %yellow_track; and %red_track; and %green_track; and not %blue_track;</Filter>">
<!ENTITY red_blue_hiking_filter "<Filter>not %yellow_track; and %red_track; and not %green_track; and %blue_track;</Filter>">
