<!--
<!ENTITY % yellow_track "(not [kct_yellow].match('^$|^bicycle$') or not [marked_trail_yellow].match('^$|^bicycle$') or [marked_trail].match('^yellow.*$'))">
<!ENTITY % red_track "(not [kct_red].match('^$|^bicycle$') or not [marked_trail_red].match('^$|^bicycle$') or [marked_trail].match('^red.*$'))">
<!ENTITY % green_track "(not [kct_green].match('^$|^bicycle$') or not [marked_trail_green].match('^$|^bicycle$') or [marked_trail].match('^green.*$'))">
<!ENTITY % blue_track "(not [kct_blue].match('^$|^bicycle$') or not [marked_trail_blue].match('^$|^bicycle$') or [marked_trail].match('^blue.*$'))">
-->
<!ENTITY % yellow_track "(not [kct_yellow].match('^$|^bicycle$'))">
<!ENTITY % red_track "(not [kct_red].match('^$|^bicycle$'))">
<!ENTITY % green_track "(not [kct_green].match('^$|^bicycle$'))">
<!ENTITY % blue_track "(not [kct_blue].match('^$|^bicycle$'))">
<!ENTITY yellow_hiking_filter "<Filter>%yellow_track; and not %red_track; and not %green_track; and not %blue_track;</Filter>">
<!ENTITY red_hiking_filter "<Filter>not %yellow_track; and %red_track; and not %green_track; and not %blue_track;</Filter>">
<!ENTITY green_hiking_filter "<Filter>not %yellow_track; and not %red_track; and %green_track; and not %blue_track;</Filter>">
<!ENTITY blue_hiking_filter "<Filter>not %yellow_track; and not %red_track; and not %green_track; and %blue_track;</Filter>">
